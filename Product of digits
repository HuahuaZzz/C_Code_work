int test()
{
int no,sum=1,rem=0;
printf("\nEnter a Number: ");
scanf("%d",&no);
while(no>0)
{
rem=no%10;
sum=sum*rem;
no=no/10;
}
printf("\nProduct of digits of a number: %d",sum);
getch();

}

